{
    "name": "jmleroux/circleci-php-client",
    "description": "CircleCI PHP client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jmleroux",
            "email": "jmleroux.pro@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*",
        "ext-json": "*",
        "webmozart/assert": "^1.9.1",
        "guzzlehttp/guzzle": "^6.5.3 || ^7.4.0",
        "phpstan/phpstan": "^0.12.99"
    },
    "require-dev": {
        "ext-sockets": "*",
        "phpunit/phpunit": "^8.5.14",
        "friendsofphp/php-cs-fixer": "^3.3.2",
        "symfony/dotenv": "^5.2.3",
        "donatj/mock-webserver": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "Jmleroux\\CircleCi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmleroux\\CircleCi\\Tests\\": "tests/"
        }
    }
}
