{
    "name": "jmleroux/circleci-php-client",
    "description": "CircleCI PHP client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jmleroux",
            "email": "jmleroux.pro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "webmozart/assert": "^1.8.0",
        "guzzlehttp/guzzle": "^6.5.3 || ^7.0.1",
        "phpstan/phpstan": "^0.12.98"
    },
    "require-dev": {
        "ext-sockets": "*",
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/dotenv": "^5.0",
        "donatj/mock-webserver": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Jmleroux\\CircleCi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmleroux\\CircleCi\\Tests\\": "tests/"
        }
    }
}
