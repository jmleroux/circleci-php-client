{
    "name": "jmleroux/circleci-php-client",
    "description": "CircleCI PHP client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jmleroux",
            "email": "jmleroux.pro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "webmozart/assert": "^1.8.0",
        "guzzlehttp/guzzle": "^6.5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "@stable",
        "symfony/dotenv": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Jmleroux\\CircleCi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmleroux\\CircleCi\\Tests\\": "tests/"
        }
    }
}
